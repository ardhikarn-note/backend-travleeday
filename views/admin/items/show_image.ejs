<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="profile-tab">
  <div class="row">
    <% for( i = 0 ; i < item.imageId.length ; i++) { %>
    <div class="col-4">
      <div class="card shadow mb-4 mt-3">
        <div class="card-body">
          <img src="/<%= item.imageId[i].imageUrl %>" alt="image" style="width: 100%; display: block">
        </div>
      </div>
    </div>
    <% } %>
  </div>
</div>
